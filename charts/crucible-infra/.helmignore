# Patterns to ignore when building packages.
# This supports shell glob matching, relative path matching, and
# negation (prefixed with !). Only one pattern per line.
.DS_Store
# Common VCS dirs
.git/
.gitignore
.bzr/
.bzrignore
.hg/
.hgignore
.svn/
# Common backup files
*.swp
*.bak
*.tmp
*.orig
*~
# Various IDEs
.project
.idea/
*.tmproj
.vscode/

# Exclude certificate and key files in the certs/ directory
# This prevents accidental inclusion of sensitive certificates and keys
certs/*.key
certs/*.crt
certs/*.pem
certs/*-dev.*
certs/*-local.*

# Exclude Keycloak realm configurations
certs/*realm*.json

# Exclude environment-specific files
certs/*.env
certs/*.local
